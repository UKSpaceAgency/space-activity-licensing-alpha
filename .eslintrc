{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "standard",
  "rules": {
    "no-undef": 0,
    "no-unused-vars": 0
  },
  "parserOptions": {
    "allowImportExportEverywhere": true,
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
